<table tal:condition="view/versions"
       class="listing">
    <tr>
        <th>Item</th>
        <th>State</th>
        <th>Owner</th>
        <th>Version</th>
    </tr>
    <tr tal:repeat="v view/versions">
        <td>
            <a tal:attributes="href v/url"
               tal:omit-tag="not:v/url">
                <span tal:replace="v/title" />
            </a>
        </td>
        <td tal:content="v/state"></td>
        <td tal:content="v/owner"></td>
        <td tal:content="v/version"></td>
    </tr>    
</table>
